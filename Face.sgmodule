#!name=面板
#!desc=自用策略组面板
#!auther= @RS0485 由 @keywos进行surge和Loon兼容性修改。复制到本地后argument=后面接本省/城市 名称拼音，或者去http://m.qiyoujiage.com/shanxi-3.shtml 查看自己省份拼音（陕西和山西比较特殊）

[Panel]
Sub_info = script-name=Sub_info9,update-interval=3600
策略面板 = script-name=网上国网,update-interval=7200
# Telecom_panel = title="电信余量",content="余量信息面板",style=info,script-name=Telecom_panel
Stream-All-Lite = script-name=Stream-All-Lite,update-interval=7200
YJ = script-name=YJ,update-interval=43200
YJ = script-name=YJ1,update-interval=43200
YJ = script-name=YJ2,update-interval=43200
FlushDNS = script-name=FlushDNS,update-interval=600

[Script]
Sub_info9 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Sub-info/Moore/Sub-info.js,script-update-interval=0,argument=url=https%3A%2F%2Fapi.flowercloud.xyz%2Fosubscribe.php%3Fsid%3D72774%26token%3DMDFhRtU4gmDZ&title=Flower&icon=link.icloud.fill&color=#FF0000&reset_day=3
Stream-All-Lite = type=generic,timeout=30,script-path=https://raw.githubusercontent.com/githubdulong/Script/master/Stream-All.js,script-update-interval=0,argument=title=流媒体解锁检测&icon=play.tv.fill&color=#D22F28
YJ = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/getsomecat/GetSomeCats/Surge/script/youjia.js,argument=hubei/wuhan
YJ1 = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/getsomecat/GetSomeCats/Surge/script/youjia.js,argument=henan/nanyang
YJ2 = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/getsomecat/GetSomeCats/Surge/script/youjia.js,argument=jiangsu/suzhou
FlushDNS = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/Rabbit-Spec/Surge/Master/Module/Panel/Flush-DNS/Moore/Flush-DNS.js,argument=title=DNS FLush&icon=cube&color=#afe59a&server=false
网上国网 = type=cron,cronexp=0 5 21 * * *,wake-system=1,script-path=https://raw.githubusercontent.com/Yuheng0101/X/main/Tasks/95598/95598.js,timeout=60
